    "import numpy as np\n",
    "\n",
    "C=-0.5+0.3j\n",
    "\n",
    "def z(n, c):\n",
    "     if n == 0:\n",
    "         return 0\n",
    "     else:\n",
    "         return z(n - 1, c) ** 2 + c\n",
    "    \n",
    "for n in range(30):\n",
    "    print(f\"|z({n})|/|C| = {np.abs(z(n, C))/np.abs(C)}\")    "
   ]
